    <main>
        <div class="pricing-container">
            <div class="pricing-header">
                <h1>Transparent Pricing for Your Project</h1>
                <p>Choose the plan that fits your needs.</p>
            </div>

            <div class="pricing-cards" id="pricingCards">
                <!-- Starter Plan -->
                <div class="pricing-card">
                    <div class="plan-icon"><i class="fa-solid fa-rocket"></i></div>
                    <h3 class="plan-name">Starter</h3>
                    <p class="plan-description">Perfect for small projects and proof of concepts</p>
                    <div class="plan-price">
                        <span class="price-currency">OMR</span><span class="price-amount" data-price="0">0</span><span class="price-period">Lifetime</span>
                        <p class="price-note">Free Lifetime</p>
                    </div>
                    <ul class="plan-features">
                        <li>Special card on the Hub</li>
                        <li>1-week of support</li>
                        <li>You can avoid setup fee of 0.500 OMR</li>
                        <li class="disabled">Independent page</li>
                    </ul>
                    <a class="btn primary" href="auth.html">Get Started</a>
                </div>

                <!-- Professional Plan -->
                <div class="pricing-card">
                    <div class="plan-icon"><i class="fa-solid fa-building"></i></div>
                    <h3 class="plan-name">Professional</h3>
                    <p class="plan-description">For large-scale projects needing advanced features</p>
                    <div class="plan-price">
                        <span class="price-currency">OMR</span><span class="price-amount" data-price="2.9">2.9</span><span class="price-period">Lifetime</span>
                        <p class="price-note">One-time payment</p>
                    </div>
                    <ul class="plan-features">
                        <li>Special card on the Hub</li>
                        <li>1-month of support</li>
                        <li>You can avoid setup fee of 0.500 OMR</li>
                        <li>Independent page</li>
                    </ul>
                    <a class="btn ghost fade" href="auth.html">Go for it!</a>
                </div>
            </div>

            <!-- Comparison Table -->
            <div class="comparison-table">
                <h2 style="text-align: center; margin-bottom: 40px;">Detailed Feature Comparison</h2>
                <table>
                    <thead>
                        <tr>
                            <th>Feature</th>
                            <th>Starter</th>
                            <th>Professional</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Special card on the Hub</td>
